<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="146.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.passwordmanager.gui.controllers.PasswordBoxController">
   <children>
      <AnchorPane layoutX="-2.0" prefHeight="219.0" prefWidth="367.0">
         <children>
            <TextField fx:id="textField" layoutX="100.0" layoutY="102.0" />
            <Button fx:id="saveBtn" layoutX="294.0" layoutY="185.0" mnemonicParsing="false" onAction="#saveNewPassword" prefHeight="27.0" prefWidth="59.0" text="Save" />
            <Button fx:id="cancelBtn" layoutX="225.0" layoutY="185.0" mnemonicParsing="false" onAction="#closeWindow" text="Cancel" />
            <Label layoutX="104.0" layoutY="77.0" text="Enter Your New Password" />
            <PasswordField fx:id="passwordField" layoutX="100.0" layoutY="102.0" />
            <CheckBox fx:id="showCheckBox" layoutX="123.0" layoutY="140.0" mnemonicParsing="false" onAction="#toggleVisiblePassword" text="Show Password" />
         </children></AnchorPane>
   </children>
</AnchorPane>
